{
  "name": "nest-experience",
  "version": "0.0.1",
  "description": "",
  "author": "DigiChanges",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "cruiser": "depcruise src",
    "start": "nest start",
    "start:dev": "nest start -w",
    "start:dev:check": "nest start --type-check -w",
    "start:debug": "nest start --type-check --debug -w",
    "start:prod": "node dist/main",
    "command": "node ./dist/command.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ts:check": "tsc -p tsconfig.build.json --noEmit",
    "pre:build": "rimraf dist",
    "pre:check": "pnpm ts:check && pnpm lint",
    "prepare": "npx husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:cov:check": "vitest run --coverage -- --CHECK",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config vitest.config.e2e.js",
    "test:ui": "vitest --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@blazity/nest-file-fastify": "^1.0.0",
    "@fastify/compress": "^8.3.0",
    "@fastify/multipart": "^9.3.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/platform-fastify": "^11.1.9",
    "@nestjs/typeorm": "^11.0.0",
    "@supabase/supabase-js": "^2.87.1",
    "dayjs": "^1.11.19",
    "dependency-cruiser": "^17.3.2",
    "fastify": "^4.29.1",
    "fastify-qs": "^5.0.0",
    "joi": "^18.0.2",
    "jwt-simple": "^0.5.6",
    "minio": "^8.0.6",
    "nest-commander": "^3.20.1",
    "nestjs-cls": "^6.1.0",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.28",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.2",
    "@types/qs": "^6.14.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^2.0.3",
    "@vitest/ui": "^2.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "pg-mem": "^3.0.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.2.0-beta.2"
  }
}
